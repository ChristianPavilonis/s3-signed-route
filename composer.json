{
    "name": "amish/laravel-s3-signed-route",
    "description": "Upload to s3 from your frontend using a pre-signed route.",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Pavilonis",
            "email": "_@christians.email",
            "homepage": "https://christianpav.com"
        }
    ],
    "homepage": "https://github.com/state/s3signedroute",
    "keywords": ["Laravel", "AWS", "S3"],
    "require": {
        "illuminate/support": "~9",
        "aws/aws-sdk-php": ">=3.225.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "State\\S3SignedRoute\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "State\\S3SignedRoute\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "State\\S3SignedRoute\\S3SignedRouteServiceProvider"
            ]
        }
    }
}
